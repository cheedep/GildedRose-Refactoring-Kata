{
    "extends": ["airbnb-base", "prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "parser": "babel"
        }
      ],
      "no-console": 0
    },
    "env": {
      "browser": false,
      "es6": true,
      "node": true,
      "mocha": true
    },
    "root": true
  }